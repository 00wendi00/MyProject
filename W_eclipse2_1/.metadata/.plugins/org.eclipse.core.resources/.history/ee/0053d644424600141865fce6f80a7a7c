package cn.edu.hainu.第十章;

/**
 * @author 张文迪
 * 类名：OneClass
 * 类说明：
 */
public interface OneClass
{
    void haode();
    class Test implements OneClass
    {
		public void haode()
		{
			System.out.println("好的~！");
		}   
		public static void main(String args[])
		{
			new Test().haode();
		}
    }
}
